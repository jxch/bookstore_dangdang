<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>bookList</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>bookList</servlet-name>
        <url-pattern>/bookList</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>bookList</servlet-name>
        <servlet-class>service.bookListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_jsp</servlet-name>
        <url-pattern>/login_jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login_jsp</servlet-name>
        <servlet-class>service.login_jsp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>service.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register_jsp</servlet-name>
        <url-pattern>/register_jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register_jsp</servlet-name>
        <servlet-class>service.register_jsp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>service.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addOrder</servlet-name>
        <url-pattern>/addOrder</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addOrder</servlet-name>
        <servlet-class>service.AddOrder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>address_from</servlet-name>
        <url-pattern>/address_from</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>address_from</servlet-name>
        <servlet-class>service.address_from</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addShoppingCart</servlet-name>
        <url-pattern>/addShoppingCart</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addShoppingCart</servlet-name>
        <servlet-class>service.addToShoppingCart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>confirmOrder</servlet-name>
        <url-pattern>/confirmOrder</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>confirmOrder</servlet-name>
        <servlet-class>service.ConfirmOrder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delShoppingCartBook</servlet-name>
        <url-pattern>/delShoppingCartBook</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delShoppingCartBook</servlet-name>
        <servlet-class>service.delBook_ShoppingCart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>shoppingCart</servlet-name>
        <url-pattern>/shoppingCart</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>shoppingCart</servlet-name>
        <servlet-class>service.shoppingCartList</servlet-class>
    </servlet>
</web-app>